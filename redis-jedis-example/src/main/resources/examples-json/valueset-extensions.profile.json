            "element": [
                {
                    "path": "effectiveDate",
                    "definition": {
                        "short": "When the value set version becomes Active and is available for use",
                        "formal": "This is the first date-time when the value set version becomes active, so this value is present on Inactive value set versions as well. The start Date_time is expected to be as of 0001 UTC of the Effective Date.",
                        "min": 0,
